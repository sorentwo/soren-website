---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Parker Selbert</title>
  <link href="http://sorentwo.com/feed.atom" rel="self" />
  <link href="http://sorentwo.com/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://sorentwo.com/</id>
  <author>
    <name>Parker Selbert</name>
    <email>parker@sorentwo.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://sorentwo.com{{ post.url }}" />
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://sorentwo.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
