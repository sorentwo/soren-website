---
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Soren</title>
  <link href="https://sorentwo.com/feed.atom" rel="self" />
  <link href="https://sorentwo.com/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>https://sorentwo.com/</id>
  <author>
    <name>Soren</name>
    <email>parker@sorentwo.com</email>
  </author>

  {% for post in site.posts limit: 30 %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="https://sorentwo.com{{ post.url }}" />
      <published>{{ post.date | date_to_xmlschema }}</published>
      <updated>{{ post.date | date_to_xmlschema }}</updated>

      <id>https://sorentwo.com{{ post.id }}</id>

      <content type="html">{{ post.content | xml_escape }}</content>
      <summary>{{ post.summary | xml_escape }}</summary>

      <author>
        <name>{{ post.author }}</name>
      </author>
    </entry>
  {% endfor %}
</feed>
